{
  "name": "cline-one",
  "version": "0.0.1",
  "description": "NestJS-based LLM Adapter Service for Cline Web Service",
  "author": "Cline Team",
  "private": true,
  "license": "MIT",
  "scripts": {
    "encrypt-config": "ts-node scripts/encrypt-config.ts",
    "generate-key": "ts-node scripts/generate-key.ts",
    "encrypt-keys": "ts-node scripts/encrypt-key.ts",
    "generate-jwt": "ts-node scripts/generate-jwt-secret.ts",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typecheck": "tsc --noEmit",
    "test:mcp": "jest test/mcp-gateway",
    "test:unit": "jest \"test/unit/.*\\.spec\\.ts$\"",
    "test:integration": "jest \"test/integration/.*\\.spec\\.ts$\"",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e"
  },
  "dependencies": {
    "@nestjs/axios": "^4.0.0",
    "@nestjs/bull": "^11.0.2",
    "@nestjs/common": "^11.0.15",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.6",
    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/platform-express": "^11.0.14",
    "@nestjs/swagger": "^11.1.1",
    "@smithery/cli": "^1.1.68",
    "@types/bull": "^4.10.4",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "bull": "^4.16.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "eventsource": "^3.0.6",
    "joi": "^17.13.3",
    "json-logic-js": "^2.0.5",
    "mongoose": "^8.13.2",
    "os-utils": "^0.0.14",
    "prom-client": "^15.1.3",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "uuid": "^11.1.0",
    "ws": "^8.18.1",
    "yaml": "^2.7.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.6",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.0.15",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.14.0",
    "@types/os-utils": "^0.0.4",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^10.1.4",
    "prettier": "^3.4.2",
    "rimraf": "^5.0.5",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.3.1",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/test/"
    ],
    "moduleNameMapper": {
      "^@core/(.*)$": "<rootDir>/src/core/$1",
      "^@interfaces/(.*)$": "<rootDir>/src/interfaces/$1",
      "^@llm/(.*)$": "<rootDir>/src/llm/$1",
      "^@mcp-gateway/(.*)$": "<rootDir>/src/mcp-gateway/$1",
      "^@controllers/(.*)$": "<rootDir>/src/controllers/$1",
      "^@services/(.*)$": "<rootDir>/src/services/$1",
      "^@test/(.*)$": "<rootDir>/test/$1"
    }
  },
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "keywords": []
}
